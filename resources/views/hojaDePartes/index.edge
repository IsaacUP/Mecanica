<div >
    <div class="col s7 push-s2">
        <h1> Lista de hojaDePartes </h1>
        <a class="waves-effect waves-light btn-large" href="/hojaDePartes/crear">Crear Nuevo</a>
        <table class="striped">
            <thead>
                <tr>
                    <th>Concepto</th>
                    <th>Cantidad</th>
                    <th>Reparaci√≥n</th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @each(hojaDeParte in hojaDePartes)
                <tr>
                    <th>{{hojaDeParte.concepto}}</th>
                    <th>{{hojaDeParte.cantidad}}</th>
                    <th>{{hojaDeParte.reparacion}}</th>
                    <th>
                        <a class="waves-effect waves-light btn" href="/facturas/crear/{{hojaDeParte.id}}"><i class="material-icons left">payment</i>Factura</a>
                        <a class="waves-effect waves-teal btn-flat" href="/hojaDePartes/editar/{{hojaDeParte.id}}">Editar</a>
                        <a class="waves-effect waves-teal btn-flat" href="/hojaDePartes/eliminar/{{hojaDeParte.id}}">Eliminar</a>
                    </th>
                </tr>
                @endeach
            </tbody>
        </table>
    </div>
</div>